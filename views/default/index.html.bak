{{extend 'pwlayout.html'}}

<div class="container">
    <form class="form-inline" action="{{=URL('index')}}" id="myForm" method="get">
        <div class="form-group">
            <label for="i_url">Insira a URL:</label>
            <input class="form-control" type="text" name="url" id="i_url">
        </div>
        <button class="btn btn-default" type="submit">Enviar</button>
    </form>
    {{if issubmit == True:}}
    <div class="container">
        <iframe src="{{=URL('static',filename)}}" width="1024" height="768" border="0"></iframe>
    </div>
    {{pass}}
</div>
